{
  "name": "react-testing-library-by-example",
  "version": "1.0.0",
  "description": "Learn how to test react and redux by sample using Jest, each of the samples contains a readme.md file that indicates the purpose of the sample plus an step by step guide to reproduce it.",
  "main": "index.js",
  "scripts": {
    "start": "env-cmd .env if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "env-cmd .env cross-env NODE_ENV=development webpack-dev-server --config ./config/webpack/app/dev.js",
    "start:prod": "env-cmd .env cross-env NODE_ENV=production webpack-dev-server --config ./config/webpack/app/prod.js",
    "build": "env-cmd .env if-env NODE_ENV=production && npm run build:prod || npm run build:dev",
    "build:dev": "rimraf dist && env-cmd .env cross-env NODE_ENV=development webpack --config ./config/webpack/app/dev.js",
    "build:prod": "rimraf dist && env-cmd .env cross-env NODE_ENV=production webpack -p --config ./config/webpack/app/prod.js",
    "test": "jest -c ./config/test/jest.json --verbose",
    "test:watch": "jest -c ./config/test/jest.json --verbose --watchAll -i"
  },
  "author": "Lemoncode",
  "repository": "https://github.com/Lemoncode/react-testing-by-example",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "bootstrap": "^4.3.1",
    "core-js": "^3.0.0",
    "history": "^4.9.0",
    "lc-form-validation": "^2.0.0",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-hot-loader": "^4.8.0",
    "react-redux": "^5.1.1",
    "react-router": "^5.0.0",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.5",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@hot-loader/react-dom": "^16.8.6",
    "@testing-library/react": "^8.0.4",
    "@testing-library/react-hooks": "^1.1.0",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.9",
    "@types/react-dom": "^16.8.3",
    "@types/react-hot-loader": "^4.1.0",
    "@types/react-redux": "^7.0.5",
    "@types/react-router": "^4.4.5",
    "@types/webpack-env": "^1.13.9",
    "awesome-typescript-loader": "^5.2.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "env-cmd": "^8.0.2",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "if-env": "^1.0.4",
    "jest": "^24.5.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "raf": "^3.4.1",
    "react-test-renderer": "^16.8.6",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.0",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.5.4",
    "tslint-react": "^4.0.0",
    "typescript": "^3.3.4000",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  }
}
